    "bcrypt": "^5.1.1",
    "clsx": "^2.0.0",
    "drizzle-orm": "^0.32.0",
    "drizzle-zod": "^0.5.1",
    "next": "^14.2.5",
    "postcss": "8.4.31",
    "react": "18.2.0",
    "react-dom": "^18.2.0",
    "tailwindcss": "3.3.3",
